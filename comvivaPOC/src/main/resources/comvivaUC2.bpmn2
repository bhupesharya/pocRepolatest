        //System.out.println("endAmountIndex is " + endAmountIndex);
        String amount = getpackapioutput.substring(startAmountIndex+8,endAmountIndex);
	   	System.out.println(" Amount is " + amount);
        l_pack.setAmount(amount); 
         
        l_packList.add(l_pack);
        System.out.println("List size is " + l_packList.size());
        l_packListString = l_packListString + i + ". " + packDesc + " " + "Validity:" + validity + " " + "Amount: Rs" + amount + "\f \r";
         
         
        // increment
         startPackIndex = endAmountIndex + 1;
         //System.out.println("startPackIndex is " + startPackIndex);
         //System.out.println("endXmlindex is " + endXmlindex);
         
        i++;
