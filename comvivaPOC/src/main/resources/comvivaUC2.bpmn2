    <bpmn2:scriptTask id="_D8995952-38DA-4173-AC35-861063CAD2A9" drools:selectable="true" name="Build Merchant Payload" scriptFormat="http://www.java.com/java">
      <bpmn2:incoming>_1D6234CF-ECCB-4621-AF7D-C1B7CC3C6C08</bpmn2:incoming>
      <bpmn2:outgoing>_BA7FBBDD-C4EB-4B7F-87AA-C0FB554323C8</bpmn2:outgoing>
      <bpmn2:script><![CDATA[// Get pack details
System.out.println("PackList is " + packList);
java.util.List l_packList = packList.getListOfPacks();
// Assigned to local variables for execute txn
String l_amount = new String();

for (int i=0;i < l_packList.size(); i++)
{
	if (((org.jbpm.comvivapoc.Pack)l_packList.get(i)).getPackNo().equals(packSelection.substring(1)))
	{
		// get other pack details
		org.jbpm.comvivapoc.Pack l_pack = (org.jbpm.comvivapoc.Pack)l_packList.get(i);
		
        //selectedPackID = l_pack.getPackID();
      kcontext.setVariable("selectedPackID", l_pack.getPackID());
      System.out.println("PackID is " + selectedPackID);
		System.out.println("PackDesc is " + l_pack.getPackDesc());
		l_amount= l_pack.getAmount();		
	}
}

String l_merchantpayload = new String();
l_merchantpayload = l_merchantpayload + "<COMMAND><TYPE>RMPREQ</TYPE><MSISDN>7799999999</MSISDN><MSISDN2>7700000125</MSISDN2><PROVIDER>101</PROVIDER><PAYID>12</PAYID><PROVIDER2>101</PROVIDER2><PAYID2>12</PAYID2><AMOUNT>";
l_merchantpayload = l_merchantpayload + l_amount + "</AMOUNT><MPIN>1357</MPIN><PIN>1357</PIN><LANGUAGE1>1</LANGUAGE1><LANGUAGE2>1</LANGUAGE2></COMMAND>";
kcontext.setVariable("merchantpayload", l_merchantpayload);
System.out.println("merchantpayload is " + merchantpayload);
//merchantpayload = l_merchantpayload;
System.out.println("merchantpayload is " + merchantpayload);
System.out.println("selectedPackID is " + selectedPackID);
]]></bpmn2:script>
