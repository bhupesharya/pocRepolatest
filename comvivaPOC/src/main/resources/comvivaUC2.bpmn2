      <bpmn2:script><![CDATA[// Extract Balance from get Balance Response
	String startBalancestr = "<BALANCE>";
	String endBalancestr = "</BALANCE>";
	int startIndex = moneyapioutput.indexOf(startBalancestr, 0);
	int endIndex = moneyapioutput.indexOf(endBalancestr, 0);
	String l_balance = moneyapioutput.substring(startIndex+9,endIndex);
	System.out.println("Balance is " + l_balance);
kcontext.setVariable("balance",l_balance);

//Prepare get Packs payload
String l_getpackpayload = "<COMMAND> <TYPE>GETPACKREQ</TYPE> <Amount>" + l_balance + "</Amount></COMMAND>";
kcontext.setVariable("getpackpayload",l_getpackpayload );
]]></bpmn2:script>
