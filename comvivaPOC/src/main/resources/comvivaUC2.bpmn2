    <bpmn2:scriptTask id="_22FE9877-B2F0-46AB-BFE0-D8800D3E5F62" drools:selectable="true" name="Extract Pack Data" scriptFormat="http://www.java.com/java">
      <bpmn2:incoming>_AEB9A970-90E8-43C9-85D3-9EF4E616DE29</bpmn2:incoming>
      <bpmn2:outgoing>_DB1E44D6-EEE9-4355-B778-6FF020ED4394</bpmn2:outgoing>
      <bpmn2:script><![CDATA[System.out.println("Extracing Pack data from " + getpackapioutput);
       int endXmlindex = getpackapioutput.lastIndexOf("</AMOUNT>");
System.out.println("last Index of Amount is " + endXmlindex);
       String startPackIDstr = "<PACKID>";
	   String endPackIDstr = "</PACKID>";
	   String startDescstr = "<DESC>";
	   String endDescstr = "</DESC>";
	   String startValiditystr = "<VALIDITY>";
	   String endValiditystr = "</VALIDITY>";
	   String startAmountstr = "<AMOUNT>";
	   String endAmountstr = "</AMOUNT>";
	   //System.out.println("last Index of Amount is " + endXmlindex);
	   int startPackIndex = 0;
       int i = 1;
       java.util.List l_packList = new java.util.ArrayList();
       String l_packListString = new String();
	   while (startPackIndex < endXmlindex) 
	   {
         org.jbpm.comvivapoc.Pack l_pack = new org.jbpm.comvivapoc.Pack();
	     l_pack.setPackNo(Integer.toString(i));
         System.out.println("pack # " + i);
	    // Get PackID
         
	   	int startpackIDIndex = getpackapioutput.indexOf(startPackIDstr, startPackIndex);
        int endpackIDIndex = getpackapioutput.indexOf(endPackIDstr, startPackIndex);
        String packID = getpackapioutput.substring(startpackIDIndex+8,endpackIDIndex);
